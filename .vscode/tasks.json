{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Compile",
      "type": "shell",
      "command": "python3 -B ${workspaceFolder}/export.py",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "Package",
      "type": "shell",
      "command": "python3 -B ${workspaceFolder}/export.py --zip",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
  ]
}
