# Variables for testing.
SRC="src"
ROOT="@ROOT_USER"
PORT="@PORT"
MAIN="$SRC/main.py"

# Variables for compilation and packaging.
FLAGS="-B"
EXPORT="export.py"

[test]
echo $PORT $ROOT
echo @PERSON1
&echo $MAIN
&echo @DEC


[compile]
&printf Compiling...\n\n
&python3 $FLAGS $EXPORT

[package]
&printf Packaging...\n\n
&python3 $FLAGS $EXPORT --zip
